# Backend environment example (safe to commit)
# Copy to backend/.env and fill in secrets where needed.

# CORS allowed origin for Angular dev server
CORS_ORIGIN=http://localhost:4200

# AI3D Python pipeline (now integrated in backend/)
PYTHON_HQ_ENABLED=true
PYTHON_CMD=./ai3d/.venv/bin/python
PYTHON_HQ_SCRIPT=./ai3d/estimate_and_mesh_optimized.py
# To enable MiDaS in Python (Torch), install deps and set true
PYTHON_HQ_USE_MIDAS=true
# Torch device: cpu | cuda | mps (Apple Silicon)
PYTORCH_DEVICE=cpu

# Firebase credentials (placeholders)
FIREBASE_API_KEY=
FIREBASE_PROJECT_ID=
FIREBASE_PRIVATE_KEY_ID=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=
FIREBASE_CLIENT_ID=
FIREBASE_CLIENT_X509_CERT_URL=
